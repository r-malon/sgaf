{{define "listItems"}}
<div class="accordion accordion-flush" id="item">
	<div class="accordion-item">
		<h2 class="accordion-header">
			<button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush{{.ID}}">
				Accordion Item #1
			</button>
		</h2>
		<div id="flush{{.ID}}" class="accordion-collapse collapse" data-bs-parent="#item">
			<div class="accordion-body"></div>
		</div>
	</div>
</div>
{{end}}
